		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВложенныйЗапрос.Организация КАК Организация,
	ВложенныйЗапрос.Склад КАК Склад,
	ВложенныйЗапрос.СчетУчета КАК СчетУчета,
	ВложенныйЗапрос.Документ КАК Документ,
	ВложенныйЗапрос.Номенклатура КАК Номенклатура,
	СУММА(ВложенныйЗапрос.СуммаНачальныйОстаток) КАК СуммаНачальныйОстаток,
	СУММА(ВложенныйЗапрос.КоличествоНачальныйОстаток) КАК КоличествоНачальныйОстаток,
	СУММА(ВложенныйЗапрос.СуммаПриход) КАК СуммаПриход,
	СУММА(ВложенныйЗапрос.КоличествоПриход) КАК КоличествоПриход,
	СУММА(ВложенныйЗапрос.СуммаРасход) КАК СуммаРасход,
	СУММА(ВложенныйЗапрос.КоличествоРасход) КАК КоличествоРасход,
	СУММА(ВложенныйЗапрос.КоличествоРасходВКормоединицах) КАК КоличествоРасходВКормоединицах,
	СУММА(ВложенныйЗапрос.СуммаКонечныйОстаток) КАК СуммаКонечныйОстаток,
	СУММА(ВложенныйЗапрос.КоличествоКонечныйОстаток) КАК КоличествоКонечныйОстаток
ИЗ
	(ВЫБРАТЬ
		ХозрасчетныйОбороты.Организация КАК Организация,
		ВЫБОР
			КОГДА ХозрасчетныйОбороты.Субконто1 ССЫЛКА Справочник.Склады
				ТОГДА ХозрасчетныйОбороты.Субконто1
			КОГДА ХозрасчетныйОбороты.Субконто2 ССЫЛКА Справочник.Склады
				ТОГДА ХозрасчетныйОбороты.Субконто2
			КОГДА ХозрасчетныйОбороты.Субконто3 ССЫЛКА Справочник.Склады
				ТОГДА ХозрасчетныйОбороты.Субконто3
		КОНЕЦ КАК Склад,
		ХозрасчетныйОбороты.Счет КАК СчетУчета,
		ХозрасчетныйОбороты.Регистратор КАК Документ,
		ВЫБОР
			КОГДА ХозрасчетныйОбороты.Субконто1 ССЫЛКА Справочник.Номенклатура
				ТОГДА ХозрасчетныйОбороты.Субконто1
			КОГДА ХозрасчетныйОбороты.Субконто2 ССЫЛКА Справочник.Номенклатура
				ТОГДА ХозрасчетныйОбороты.Субконто2
			КОГДА ХозрасчетныйОбороты.Субконто3 ССЫЛКА Справочник.Номенклатура
				ТОГДА ХозрасчетныйОбороты.Субконто3
		КОНЕЦ КАК Номенклатура,
		NULL КАК СуммаНачальныйОстаток,
		NULL КАК КоличествоНачальныйОстаток,
		ЕСТЬNULL(ХозрасчетныйОбороты.СуммаОборотДт, 0) КАК СуммаПриход,
		ЕСТЬNULL(ХозрасчетныйОбороты.КоличествоОборотДт, 0) КАК КоличествоПриход,
		ЕСТЬNULL(ХозрасчетныйОбороты.СуммаОборотКт, 0) КАК СуммаРасход,
		ЕСТЬNULL(ХозрасчетныйОбороты.КоличествоОборотКт, 0) КАК КоличествоРасход,
		ЕСТЬNULL(ХозрасчетныйОбороты.КоличествоОборотКт, 0) * ЕСТЬNULL(ВложенныйЗапрос.Коэффициент, 0) КАК КоличествоРасходВКормоединицах,
		NULL КАК СуммаКонечныйОстаток,
		NULL КАК КоличествоКонечныйОстаток
	ИЗ
		РегистрБухгалтерии.Хозрасчетный.ОстаткиИОбороты(
				&amp;НачалоПериода {(&amp;НачалоПериода)},
				&amp;КонецПериода {(&amp;КонецПериода)},
				Регистратор,
				,
				,
				&amp;ВидыСубконто,
				ВЫБОР
					КОГДА Субконто1 ССЫЛКА Справочник.Номенклатура
								И НЕ Субконто1.Услуга
							ИЛИ Субконто2 ССЫЛКА Справочник.Номенклатура
								И НЕ Субконто2.Услуга
							ИЛИ Субконто3 ССЫЛКА Справочник.Номенклатура
								И НЕ Субконто3.Услуга
						ТОГДА ИСТИНА
					ИНАЧЕ ЛОЖЬ
				КОНЕЦ) КАК ХозрасчетныйОбороты
			ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
				НоменклатураЕдиницыИзмерения.Ссылка КАК Номенклатура,
				МАКСИМУМ(НоменклатураЕдиницыИзмерения.Коэффициент) КАК Коэффициент
			ИЗ
				Справочник.Номенклатура.ЕдиницыИзмерения КАК НоменклатураЕдиницыИзмерения
			ГДЕ
				НоменклатураЕдиницыИзмерения.ЕдиницаИзмерения = ЗНАЧЕНИЕ(Справочник.КлассификаторЕдиницИзмерения.Кормоединица)
			
			СГРУППИРОВАТЬ ПО
				НоменклатураЕдиницыИзмерения.Ссылка) КАК ВложенныйЗапрос
			ПО ХозрасчетныйОбороты.Субконто1 = ВложенныйЗапрос.Номенклатура
	ГДЕ
		(ХозрасчетныйОбороты.СуммаОборотДт &lt;&gt; 0
				ИЛИ ХозрасчетныйОбороты.СуммаОборотКт &lt;&gt; 0)
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		ХозрасчетныйОстаткиНач.Организация,
		ВЫБОР
			КОГДА ХозрасчетныйОстаткиНач.Субконто1 ССЫЛКА Справочник.Склады
				ТОГДА ХозрасчетныйОстаткиНач.Субконто1
			КОГДА ХозрасчетныйОстаткиНач.Субконто2 ССЫЛКА Справочник.Склады
				ТОГДА ХозрасчетныйОстаткиНач.Субконто2
			КОГДА ХозрасчетныйОстаткиНач.Субконто3 ССЫЛКА Справочник.Склады
				ТОГДА ХозрасчетныйОстаткиНач.Субконто3
		КОНЕЦ,
		ХозрасчетныйОстаткиНач.Счет,
		NULL,
		ВЫБОР
			КОГДА ХозрасчетныйОстаткиНач.Субконто1 ССЫЛКА Справочник.Номенклатура
				ТОГДА ХозрасчетныйОстаткиНач.Субконто1
			КОГДА ХозрасчетныйОстаткиНач.Субконто2 ССЫЛКА Справочник.Номенклатура
				ТОГДА ХозрасчетныйОстаткиНач.Субконто2
			КОГДА ХозрасчетныйОстаткиНач.Субконто3 ССЫЛКА Справочник.Номенклатура
				ТОГДА ХозрасчетныйОстаткиНач.Субконто3
		КОНЕЦ,
		СУММА(ЕСТЬNULL(ХозрасчетныйОстаткиНач.СуммаОстаток, 0)),
		СУММА(ЕСТЬNULL(ХозрасчетныйОстаткиНач.КоличествоОстаток, 0)),
		NULL,
		NULL,
		NULL,
		NULL,
		NULL,
		NULL,
		NULL
	ИЗ
		РегистрБухгалтерии.Хозрасчетный.Остатки(
				&amp;НачалоПериода {(&amp;НачалоПериода)},
				,
				&amp;ВидыСубконто,
				ВЫБОР
					КОГДА Субконто1 ССЫЛКА Справочник.Номенклатура
								И НЕ Субконто1.Услуга
							ИЛИ Субконто2 ССЫЛКА Справочник.Номенклатура
								И НЕ Субконто2.Услуга
							ИЛИ Субконто3 ССЫЛКА Справочник.Номенклатура
								И НЕ Субконто3.Услуга
						ТОГДА ИСТИНА
					ИНАЧЕ ЛОЖЬ
				КОНЕЦ) КАК ХозрасчетныйОстаткиНач
	
	СГРУППИРОВАТЬ ПО
		ХозрасчетныйОстаткиНач.Организация,
