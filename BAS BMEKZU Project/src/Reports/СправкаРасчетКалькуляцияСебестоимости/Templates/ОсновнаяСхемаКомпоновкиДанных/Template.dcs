		<field xsi:type="DataSetFieldFolder">
			<dataPath>Ресурсы</dataPath>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Ресурсы</v8:content>
				</v8:item>
				<v8:item>
