		<field xsi:type="DataSetFieldField">
			<dataPath>ВоинскийУчет</dataPath>
			<field>ВоинскийУчет</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Воинский учет</v8:content>
				</v8:item>
				<v8:item>
