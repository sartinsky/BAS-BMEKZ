		<query>ВЫБРАТЬ
	СУММА(ЕСТЬNULL(ИНАГРО_РеестрТТНВвоз.ЗачетныйВес, 0)) КАК ОстатокНаДатуНачалаЗачВес,
	СУММА(ЕСТЬNULL(ИНАГРО_РеестрТТНВвоз.ФизическийВес, 0)) КАК ОстатокНаДатуНачалаФизВес,
	ИНАГРО_РеестрТТНВвоз.Номенклатура КАК Номенклатура
ПОМЕСТИТЬ втОстаток
ИЗ
	Документ.ИНАГРО_РеестрТТНВвоз КАК ИНАГРО_РеестрТТНВвоз
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ИНАГРО_ВидыУрожая КАК ИНАГРО_ВидыУрожая
		ПО ИНАГРО_РеестрТТНВвоз.Урожай = ИНАГРО_ВидыУрожая.Ссылка
ГДЕ
	ИНАГРО_РеестрТТНВвоз.Проведен = ИСТИНА
	И ИНАГРО_РеестрТТНВвоз.Дата &lt; &amp;ДатаНач
	И ИНАГРО_РеестрТТНВвоз.Дата &gt;= ИНАГРО_ВидыУрожая.ДатаНачала

СГРУППИРОВАТЬ ПО
	ИНАГРО_РеестрТТНВвоз.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	НАЧАЛОПЕРИОДА(ИНАГРО_РеестрТТНВвоз.Дата, ДЕНЬ) КАК День,
	ЕСТЬNULL(втОстаток.ОстатокНаДатуНачалаЗачВес, 0) КАК ОстатокНаНачалоЗачВес,
	ЕСТЬNULL(втОстаток.ОстатокНаДатуНачалаФизВес, 0) КАК ОстатокНаНачалоФизВес,
	ИНАГРО_РеестрТТНВвоз.Номенклатура КАК Номенклатура,
	ИНАГРО_РеестрТТНВвоз.Урожай КАК Урожай,
	ЕСТЬNULL(ИНАГРО_РеестрТТНВвоз.ЗачетныйВес, 0) КАК ПриходЗачВес,
	ЕСТЬNULL(ИНАГРО_РеестрТТНВвоз.ФизическийВес, 0) КАК ПриходФизВес,
	ЕСТЬNULL(втОстаток.ОстатокНаДатуНачалаЗачВес, 0) КАК ОстатокНаКонецЗачВес,
	ЕСТЬNULL(втОстаток.ОстатокНаДатуНачалаФизВес, 0) КАК ОстатокНаКонецФизВес,
	ИНАГРО_РеестрТТНВвоз.Владелец КАК Владелец,
	ИНАГРО_РеестрТТНВвоз.Организация КАК Организация,
	ИНАГРО_РеестрТТНВвоз.ФизическийВес КАК ФизическийВес,
	ИНАГРО_РеестрТТНВвоз.ЗачетныйВес КАК ЗачетныйВес
{ВЫБРАТЬ
	Владелец.*}
ИЗ
	Документ.ИНАГРО_РеестрТТНВвоз КАК ИНАГРО_РеестрТТНВвоз
		ЛЕВОЕ СОЕДИНЕНИЕ втОстаток КАК втОстаток
		ПО (втОстаток.Номенклатура = ИНАГРО_РеестрТТНВвоз.Номенклатура)
ГДЕ
	ИНАГРО_РеестрТТНВвоз.Проведен = ИСТИНА
	И ИНАГРО_РеестрТТНВвоз.Дата &gt;= &amp;ДатаНач
	И ИНАГРО_РеестрТТНВвоз.Дата &lt;= &amp;ДатаКон</query>
