	КомандаПечати.Идентификатор = "ПропускФ196_2021";
	КомандаПечати.Представление = НСтр("ru='Пропуск ф196 (2021)';uk='Перепустка ф196 (2021)'");
   	КомандаПечати.Обработчик    = "УправлениеПечатьюБПКлиент.ВыполнитьКомандуПечати";
