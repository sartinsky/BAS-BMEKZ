				МинС = Строка(Мин);
			Иначе
				МинС = "0"+Строка(Мин);
			КонецЕсли;	
			Если СтрДлина(Строка(Сек)) = 2 Тогда
				СекС = Строка(Сек);
			Иначе
				СекС = "0"+Строка(Сек);
