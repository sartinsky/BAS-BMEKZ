		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	РасходнаяНакладная.Дата КАК Дата,
	" від " КАК ДоговорДатаТекст,
	ЕСТЬNULL(РасходнаяНакладная.Договор.ДатаДоговора, ДАТАВРЕМЯ(1, 1, 1)) КАК ДоговорДата,
	РасходнаяНакладная.Договор.НомерДоговора КАК ДоговорНомер,
	ЕСТЬNULL(РасходнаяНакладная.Договор.Наименование, "") КАК ДоговорНомерТекст,
