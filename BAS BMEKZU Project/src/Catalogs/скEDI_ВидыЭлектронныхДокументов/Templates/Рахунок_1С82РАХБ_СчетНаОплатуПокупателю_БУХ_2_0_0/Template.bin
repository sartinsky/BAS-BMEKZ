		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	СчетНаОплатуПокупателю.Дата КАК Дата,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.Сумма, 0) КАК СуммаДокумента,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.Сумма, 0) КАК СуммаДокументаТекст,
	ВЫБОР
		КОГДА СчетНаОплатуПокупателю.ДоговорКонтрагента.НаименованиеДляПечати = ""
			ТОГДА ВЫБОР
					КОГДА СчетНаОплатуПокупателю.ДоговорКонтрагента.Номер = ""
						ТОГДА СчетНаОплатуПокупателю.ДоговорКонтрагента.Наименование
					ИНАЧЕ "№"
				КОНЕЦ
		ИНАЧЕ СчетНаОплатуПокупателю.ДоговорКонтрагента.НаименованиеДляПечати
	КОНЕЦ КАК ДоговорНомерТекст,
	ВЫБОР
		КОГДА СчетНаОплатуПокупателю.ДоговорКонтрагента.НаименованиеДляПечати = ""
			ТОГДА СчетНаОплатуПокупателю.ДоговорКонтрагента.Номер
		ИНАЧЕ ""
	КОНЕЦ КАК ДоговорНомер,
	ВЫБОР
		КОГДА СчетНаОплатуПокупателю.ДоговорКонтрагента.НаименованиеДляПечати = ""
			ТОГДА ВЫБОР
					КОГДА СчетНаОплатуПокупателю.ДоговорКонтрагента.Дата = ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА ""
					ИНАЧЕ "від"
				КОНЕЦ
		ИНАЧЕ ""
	КОНЕЦ КАК ДоговорДатаТекст,
	ВЫБОР
		КОГДА СчетНаОплатуПокупателю.ДоговорКонтрагента.НаименованиеДляПечати = ""
			ТОГДА СчетНаОплатуПокупателю.ДоговорКонтрагента.Дата
		ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
	КОНЕЦ КАК ДоговорДата,
	"Договір:" КАК ДоговорТекст,
	ЕСТЬNULL(ЮрАдресОрганизации.Представление, "") КАК ЮрАдресОрганизации,
	ЕСТЬNULL(КодыОрганизаций.ИНН, "") КАК ИНН,
	ЕСТЬNULL(ОрганизацияEDI.Код, "") КАК КодПоЕДРПОУ,
	ЕСТЬNULL(КодыОрганизаций.НомерСвидетельства, "") КАК НомерСвидетельства,
	СчетНаОплатуПокупателю.Организация.НаименованиеПолное КАК ОрганизацияНаименование,
	ЕСТЬNULL(ТелефонПоЮрАдресуОрганизации.Представление, "") КАК ТелефонПоЮрАдресуОрганизации,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.Номенклатура, 0) КАК КоличествоНоменклатур,
	СчетНаОплатуПокупателю.Ссылка,
	ЕСТЬNULL(ЮрАдресКонтрагента.Представление, "") КАК ЮрАресКонтрагента,
	ЕСТЬNULL(ТелефонПоЮрАдресуКонтрагента.Представление, "") КАК ТелефонПоЮрАдресуКонтрагент,
	СчетНаОплатуПокупателю.Контрагент.НаименованиеПолное КАК НаименованиеКонтрагента,
	СчетНаОплатуПокупателю.Контрагент.КодФилиала КАК КодФилиалаКонтрагента,
	СчетНаОплатуПокупателю.Контрагент.КодПоЕДРПОУ КАК КодЕДРПОУКонтрагента,
	ЕСТЬNULL(СчетНаОплатуПокупателю.Склад.Наименование, "") КАК Склад,
	ВЫБОР
		КОГДА ЕСТЬNULL(СчетНаОплатуПокупателю.Склад.Наименование, "") = ""
			ТОГДА ""
		ИНАЧЕ "Склад:"
	КОНЕЦ КАК СкладТекст,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаНДС, 0) КАК СуммаНДС,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаНДС, 0) КАК СуммаНДСТекст,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаБезНДС, 0) КАК СуммаБезНДС,
	"код за ЄДРПОУ " КАК ЕДРПОУОрганизацииТекст,
	"код за ЄДРПОУ " КАК ЕДРПОУКонтрагентаТекст,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаСкидки, 0) КАК СуммаСкидки,
	ЕСТЬNULL(УчетнаяПолитика.СхемаНалогообложенияНаименование, "") КАК СхемаНалогообложения,
	ЕСТЬNULL(ОрганизацияEDI.Филиал, "") КАК ФилиалОрганизации,
	ВЫБОР
		КОГДА СчетНаОплатуПокупателю.СтруктурнаяЕдиница.Банк ЕСТЬ NULL
			ТОГДА ЕСТЬNULL(СчетНаОплатуПокупателю.Организация.ОсновнойБанковскийСчет.Банк.Код, "")
		ИНАЧЕ ЕСТЬNULL(СчетНаОплатуПокупателю.СтруктурнаяЕдиница.Банк.Код, "")
	КОНЕЦ КАК МФОБанка,
	ВЫБОР
		КОГДА СчетНаОплатуПокупателю.СтруктурнаяЕдиница.Банк ЕСТЬ NULL
			ТОГДА ЕСТЬNULL(СчетНаОплатуПокупателю.Организация.ОсновнойБанковскийСчет.Банк.Наименование, "")
		ИНАЧЕ ЕСТЬNULL(СчетНаОплатуПокупателю.СтруктурнаяЕдиница.Банк.Наименование, "")
	КОНЕЦ КАК НаименованиеБанка,
	ВЫБОР
		КОГДА СчетНаОплатуПокупателю.СтруктурнаяЕдиница.Банк ЕСТЬ NULL
			ТОГДА ЕСТЬNULL(СчетНаОплатуПокупателю.Организация.ОсновнойБанковскийСчет.НомерСчета, "")
		ИНАЧЕ ЕСТЬNULL(СчетНаОплатуПокупателю.СтруктурнаяЕдиница.НомерСчета, "")
	КОНЕЦ КАК РасчетныйСчет,
	ЕСТЬNULL(СчетНаОплатуПокупателю.Ответственный.Наименование, "") КАК Ответственный,
	СчетНаОплатуПокупателю.Номер,
	СчетНаОплатуПокупателю.Организация,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаБезНДС, 0) КАК СуммаБезНДС_текст
ИЗ
	Документ.СчетНаОплатуПокупателю КАК СчетНаОплатуПокупателю
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			КонтактнаяИнформация.Ссылка КАК Объект,
			КонтактнаяИнформация.Представление КАК Представление
		ИЗ
			Справочник.Организации.КонтактнаяИнформация КАК КонтактнаяИнформация
		ГДЕ
			КонтактнаяИнформация.Тип = ЗНАЧЕНИЕ(Перечисление.ТипыКонтактнойИнформации.Адрес)
			И КонтактнаяИнформация.Вид = ЗНАЧЕНИЕ(Справочник.ВидыКонтактнойИнформации.ЮрАдресОрганизации)) КАК ЮрАдресОрганизации
		ПО СчетНаОплатуПокупателю.Организация = ЮрАдресОрганизации.Объект
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			КодыОрганизацииСрезПоследних.Организация КАК Организация,
			КодыОрганизацииСрезПоследних.ИНН КАК ИНН,
			КодыОрганизацииСрезПоследних.НомерСвидетельства КАК НомерСвидетельства
		ИЗ
			РегистрСведений.КодыОрганизации.СрезПоследних(, ) КАК КодыОрганизацииСрезПоследних) КАК КодыОрганизаций
		ПО СчетНаОплатуПокупателю.Организация = КодыОрганизаций.Организация
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			КонтактнаяИнформация.Ссылка КАК Объект,
			КонтактнаяИнформация.Представление КАК Представление
		ИЗ
			Справочник.Организации.КонтактнаяИнформация КАК КонтактнаяИнформация
		ГДЕ
			КонтактнаяИнформация.Тип = ЗНАЧЕНИЕ(Перечисление.ТипыКонтактнойИнформации.Телефон)
			И КонтактнаяИнформация.Вид = ЗНАЧЕНИЕ(Справочник.ВидыКонтактнойИнформации.ТелефонОрганизации)) КАК ТелефонПоЮрАдресуОрганизации
		ПО СчетНаОплатуПокупателю.Организация = ТелефонПоЮрАдресуОрганизации.Объект
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ВложенныйЗапрос.Ссылка КАК Ссылка,
			СУММА(ВложенныйЗапрос.КоличествоНоменклатур) КАК Номенклатура,
			СУММА(ВложенныйЗапрос.СуммаНДС) КАК СуммаНДС,
			СУММА(ВложенныйЗапрос.СуммаСкидки) КАК СуммаСкидки,
			СУММА(ВложенныйЗапрос.Сумма) КАК Сумма,
			СУММА(ВложенныйЗапрос.СуммаБезНДС) КАК СуммаБезНДС
		ИЗ
			(ВЫБРАТЬ
				СчетНаОплатуПокупателюТовары.Ссылка КАК Ссылка,
				КОЛИЧЕСТВО(РАЗЛИЧНЫЕ СчетНаОплатуПокупателюТовары.Номенклатура) КАК КоличествоНоменклатур,
				СУММА(СчетНаОплатуПокупателюТовары.СуммаНДС) КАК СуммаНДС,
				СУММА(СчетНаОплатуПокупателюТовары.СуммаСкидки) КАК СуммаСкидки,
				СчетНаОплатуПокупателюТовары.ЕдиницаИзмерения КАК ЕдиницаИзмерения,
				СчетНаОплатуПокупателюТовары.Цена КАК Цена,
				СУММА(ВЫБОР
						КОГДА СчетНаОплатуПокупателюТовары.Ссылка.СуммаВключаетНДС
							ТОГДА СчетНаОплатуПокупателюТовары.Сумма
						ИНАЧЕ СчетНаОплатуПокупателюТовары.Сумма + СчетНаОплатуПокупателюТовары.СуммаНДС
					КОНЕЦ) КАК Сумма,
				СУММА(ВЫБОР
						КОГДА СчетНаОплатуПокупателюТовары.Ссылка.СуммаВключаетНДС
							ТОГДА СчетНаОплатуПокупателюТовары.Сумма - СчетНаОплатуПокупателюТовары.СуммаНДС
						ИНАЧЕ СчетНаОплатуПокупателюТовары.Сумма
					КОНЕЦ) КАК СуммаБезНДС
			ИЗ
				Документ.СчетНаОплатуПокупателю.Товары КАК СчетНаОплатуПокупателюТовары
			ГДЕ
				СчетНаОплатуПокупателюТовары.Ссылка = &amp;Ссылка
			
			СГРУППИРОВАТЬ ПО
				СчетНаОплатуПокупателюТовары.Ссылка,
				СчетНаОплатуПокупателюТовары.ЕдиницаИзмерения,
				СчетНаОплатуПокупателюТовары.Цена
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				СчетНаОплатуПокупателюУслуги.Ссылка,
				КОЛИЧЕСТВО(РАЗЛИЧНЫЕ СчетНаОплатуПокупателюУслуги.Номенклатура),
				СУММА(СчетНаОплатуПокупателюУслуги.СуммаНДС),
				СУММА(СчетНаОплатуПокупателюУслуги.СуммаСкидки),
				СчетНаОплатуПокупателюУслуги.Номенклатура.БазоваяЕдиницаИзмерения,
				СчетНаОплатуПокупателюУслуги.Цена,
				СУММА(ВЫБОР
						КОГДА СчетНаОплатуПокупателюУслуги.Ссылка.СуммаВключаетНДС
							ТОГДА СчетНаОплатуПокупателюУслуги.Сумма
						ИНАЧЕ СчетНаОплатуПокупателюУслуги.Сумма + СчетНаОплатуПокупателюУслуги.СуммаНДС
					КОНЕЦ),
				СУММА(ВЫБОР
						КОГДА СчетНаОплатуПокупателюУслуги.Ссылка.СуммаВключаетНДС
							ТОГДА СчетНаОплатуПокупателюУслуги.Сумма - СчетНаОплатуПокупателюУслуги.СуммаНДС
						ИНАЧЕ СчетНаОплатуПокупателюУслуги.Сумма
					КОНЕЦ)
			ИЗ
				Документ.СчетНаОплатуПокупателю.Услуги КАК СчетНаОплатуПокупателюУслуги
			ГДЕ
				СчетНаОплатуПокупателюУслуги.Ссылка = &amp;Ссылка
			
			СГРУППИРОВАТЬ ПО
				СчетНаОплатуПокупателюУслуги.Ссылка,
				СчетНаОплатуПокупателюУслуги.Цена,
				СчетНаОплатуПокупателюУслуги.Номенклатура.БазоваяЕдиницаИзмерения) КАК ВложенныйЗапрос
		
		СГРУППИРОВАТЬ ПО
			ВложенныйЗапрос.Ссылка) КАК ДанныеПоТабличнойЧасти
		ПО СчетНаОплатуПокупателю.Ссылка = ДанныеПоТабличнойЧасти.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			КонтактнаяИнформация.Ссылка КАК Объект,
			КонтактнаяИнформация.Представление КАК Представление
		ИЗ
			Справочник.Контрагенты.КонтактнаяИнформация КАК КонтактнаяИнформация
		ГДЕ
			КонтактнаяИнформация.Тип = ЗНАЧЕНИЕ(Перечисление.ТипыКонтактнойИнформации.Адрес)
			И КонтактнаяИнформация.Вид = ЗНАЧЕНИЕ(Справочник.ВидыКонтактнойИнформации.ЮрАдресКонтрагента)) КАК ЮрАдресКонтрагента
		ПО СчетНаОплатуПокупателю.Контрагент = ЮрАдресКонтрагента.Объект
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			КонтактнаяИнформация.Ссылка КАК Объект,
			КонтактнаяИнформация.Представление КАК Представление
		ИЗ
			Справочник.Контрагенты.КонтактнаяИнформация КАК КонтактнаяИнформация
		ГДЕ
			КонтактнаяИнформация.Тип = ЗНАЧЕНИЕ(Перечисление.ТипыКонтактнойИнформации.Телефон)
			И КонтактнаяИнформация.Вид = ЗНАЧЕНИЕ(Справочник.ВидыКонтактнойИнформации.ТелефонКонтрагента)) КАК ТелефонПоЮрАдресуКонтрагента
		ПО СчетНаОплатуПокупателю.Контрагент = ТелефонПоЮрАдресуКонтрагента.Объект
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
