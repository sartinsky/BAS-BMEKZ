					<tl>
						<v8:item>
							<v8:lang>ru</v8:lang>
							<v8:content>// ищем контрагента по коду ЕДРПОУ, если не находим - создаем контрагента, заполняем контактную информацию.

КодЕДРПОУ 				= ДанныеТаблицы.Получить("FIRM_EDRPOU");
НаименованиеКонтрагента = ДанныеТаблицы.Получить("FIRM_NAME");
ИНН						= ДанныеТаблицы.Получить("FIRM_INN");
НомерСвидетельства		= ДанныеТаблицы.Получить("FIRM_SRPNDS");
КодФилиала 				= ДанныеТаблицы.Получить("FIRM_DEPT");
ЮрАдресКонтрагента		= ДанныеТаблицы.Получить("FIRM_ADR");
ТелефонКонтрагента		= ДанныеТаблицы.Получить("FIRM_TELEFON");

НаименованиеГруппы = "Созданные при загрузке электронных документов";
Запрос = Новый Запрос;
Запрос.Текст = 
"ВЫБРАТЬ ПЕРВЫЕ 1
|	Контрагенты.Ссылка
|ИЗ
|	Справочник.Контрагенты КАК Контрагенты
|ГДЕ
|	Контрагенты.Наименование = &amp;НаименованиеГруппы
|	И НЕ Контрагенты.ПометкаУдаления
|	И Контрагенты.ЭтоГруппа";

Запрос.УстановитьПараметр("НаименованиеГруппы", НаименованиеГруппы);
Выборка = Запрос.Выполнить().Выбрать();

Если Выборка.Следующий() Тогда 
	Группа = Выборка.Ссылка;
Иначе
	ГруппаОбъект = Справочники.Контрагенты.СоздатьГруппу();
	ГруппаОбъект.Наименование = НаименованиеГруппы;
	ГруппаОбъект.Записать();
	Группа = ГруппаОбъект.Ссылка;
КонецЕсли;

Контрагент = Справочники.Контрагенты.НайтиПоРеквизиту("КодПоЕДРПОУ", КодЕДРПОУ);
Если Контрагент = Справочники.Контрагенты.ПустаяСсылка() Тогда
	КонтрагентОбъект = Справочники.Контрагенты.СоздатьЭлемент();
	КонтрагентОбъект.Наименование 		= НаименованиеКонтрагента;
	КонтрагентОбъект.НаименованиеПолное = НаименованиеКонтрагента;
	КонтрагентОбъект.КодПоЕДРПОУ	 	= КодЕДРПОУ;
	КонтрагентОбъект.ИНН 				= ИНН;
	КонтрагентОбъект.НомерСвидетельства = НомерСвидетельства;
	КонтрагентОбъект.КодФилиала 		= КодФилиала;
	КонтрагентОбъект.ЮрФизЛицо 			= ?(СтрДлина(ИНН) &gt; 8, Перечисления.ЮрФизЛицо.ФизЛицо, Перечисления.ЮрФизЛицо.ЮрЛицо);
	КонтрагентОбъект.Комментарий		= "Автоматически создана при загрузке электронных документов";
	КонтрагентОбъект.Родитель			= Группа;
	КонтрагентОбъект.Записать();
	Контрагент = КонтрагентОбъект.Ссылка;
КонецЕсли;

Результат = Контрагент;

КонтактнаяИнформация = РегистрыСведений.КонтактнаяИнформация.СоздатьМенеджерЗаписи();
КонтактнаяИнформация.Объект = Результат;
КонтактнаяИнформация.Вид	= Справочники.ВидыКонтактнойИнформации.ЮрАдресКонтрагента;
КонтактнаяИнформация.Тип	= Перечисления.ТипыКонтактнойИнформации.Адрес;
КонтактнаяИнформация.Представление = ЮрАдресКонтрагента;
КонтактнаяИнформация.Записать(Истина);

КонтактнаяИнформация = РегистрыСведений.КонтактнаяИнформация.СоздатьМенеджерЗаписи();
КонтактнаяИнформация.Объект = Результат;
КонтактнаяИнформация.Вид	= Справочники.ВидыКонтактнойИнформации.ТелефонКонтрагента;
КонтактнаяИнформация.Тип	= Перечисления.ТипыКонтактнойИнформации.Телефон;
КонтактнаяИнформация.Представление = ТелефонКонтрагента;
КонтактнаяИнформация.Записать(Истина);
</v8:content>
