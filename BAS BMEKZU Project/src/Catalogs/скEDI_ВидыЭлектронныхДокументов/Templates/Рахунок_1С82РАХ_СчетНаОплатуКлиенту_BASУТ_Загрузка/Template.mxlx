			<c>
				<c>
					<f>45</f>
					<tl>
						<v8:item>
							<v8:lang>ru</v8:lang>
							<v8:content>// в данных электронного документа есть наименование номеклатуры и артикул.
// Алгоритм:
// Ищем в РС "Номенклатура контрагентов" по контрагенту, артикулу или наименованию и признаку "Услуга"=Ложь.
// Если не нашли, то ищем в справочнике "Номенклатура" по артикулу или наименованию и признаку "Услуга"=Ложь.
// Если не нашли - создаем новый элемент справочника "Номенклатура", помещаем в каталог "Созданная при загрузке электронных документов (Товары)" 
// и устанавливаем связь в РС "Номенклатура контрагентов".
Артикул 		= ДанныеТаблицы.Получить("TAB1_NOMENKLATURA_NUM");
Наименование 	= ДанныеТаблицы.Получить("TAB1_NOMENKLATURA_NAME");
Контрагент      = РеквизитыДокумента0.Получить("Контрагент");
Результат 		= Неопределено;
ТипНоменклатуры = ДанныеТаблицы.Получить("TAB1_T");

Если НЕ ЗначениеЗаполнено(Артикул) И НЕ ЗначениеЗаполнено(Наименование) Тогда
	Результат = Справочники.Номенклатура.ПустаяСсылка();
Иначе
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	ВидыНоменклатуры.Ссылка
	|ИЗ
	|	Справочник.ВидыНоменклатуры КАК ВидыНоменклатуры
	|ГДЕ
	|	ВидыНоменклатуры.ТипНоменклатуры = ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Товар)
	|	И ВидыНоменклатуры.Наименование = ""Товар""";
	Если ТипНоменклатуры = "П" Тогда
		Запрос.Текст = СтрЗаменить(Запрос.Текст, "ВидыНоменклатуры.Наименование = ""Товар""", "ВидыНоменклатуры.Наименование = ""Услуга""");
		Запрос.Текст = СтрЗаменить(Запрос.Текст, "ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Товар)", "ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Услуга)");
	КонецЕсли;
	Выборка = Запрос.Выполнить().Выбрать();
	
	Если Выборка.Следующий() Тогда
		
		ВидНоменклатуры = Выборка.Ссылка;
		Запрос = Новый Запрос;
		Запрос.Текст = 
		"ВЫБРАТЬ ПЕРВЫЕ 1
		|	НоменклатураКонтрагентов.Номенклатура,
		|	ВЫБОР
		|		КОГДА НоменклатураКонтрагентов.АртикулНоменклатурыКонтрагента = &amp;Артикул
		|			ТОГДА 1000
		|		КОГДА НоменклатураКонтрагентов.НаименованиеНоменклатурыКонтрагента ПОДОБНО &amp;Наименование
		|			ТОГДА 100
		|		ИНАЧЕ 0
		|	КОНЕЦ КАК Порядок
		|ИЗ
		|	РегистрСведений.НоменклатураКонтрагентов КАК НоменклатураКонтрагентов
		|ГДЕ
		|	НоменклатураКонтрагентов.Контрагент.КодПоЕДРПОУ = &amp;Контрагент
		|	И (НоменклатураКонтрагентов.АртикулНоменклатурыКонтрагента = &amp;Артикул
		|			ИЛИ НоменклатураКонтрагентов.НаименованиеНоменклатурыКонтрагента ПОДОБНО &amp;Наименование)
		|	И НЕ НоменклатураКонтрагентов.Номенклатура.ЭтоГруппа
		|	И НЕ НоменклатураКонтрагентов.Номенклатура.ПометкаУдаления
		|	И НоменклатураКонтрагентов.Номенклатура.ВидНоменклатуры = &amp;ВидНоменклатуры
		|
		|УПОРЯДОЧИТЬ ПО
		|	Порядок УБЫВ";
		Запрос.УстановитьПараметр("Артикул"		, Артикул);
		Запрос.УстановитьПараметр("Наименование", Наименование);
		Запрос.УстановитьПараметр("Контрагент"	, Контрагент);
		Запрос.УстановитьПараметр("ВидНоменклатуры", ВидНоменклатуры);
		
		Выборка = Запрос.Выполнить().Выбрать();
		
		Если Выборка.Следующий() Тогда
			Результат = Выборка.Номенклатура;
		Иначе 
			Запрос = Новый Запрос;
			Запрос.Текст = 
			"ВЫБРАТЬ ПЕРВЫЕ 1
			|	ВЫБОР
			|		КОГДА Номенклатура1.Артикул = &amp;Артикул
			|			ТОГДА 1000
			|		КОГДА Номенклатура1.НаименованиеПолное ПОДОБНО &amp;Наименование
			|			ТОГДА 100
			|		ИНАЧЕ 0
			|	КОНЕЦ КАК Порядок,
			|	Номенклатура1.Ссылка
			|ИЗ
			|	Справочник.Номенклатура КАК Номенклатура1
			|ГДЕ
			|	(Номенклатура1.Артикул = &amp;Артикул
			|			ИЛИ Номенклатура1.НаименованиеПолное ПОДОБНО &amp;Наименование)
			|	И НЕ Номенклатура1.ЭтоГруппа
			|	И НЕ Номенклатура1.ПометкаУдаления
			|	И Номенклатура1.ВидНоменклатуры = &amp;ВидНоменклатуры
			|
			|УПОРЯДОЧИТЬ ПО
			|	Порядок УБЫВ";
			Запрос.УстановитьПараметр("Артикул"		, Артикул);
			Запрос.УстановитьПараметр("Наименование", Наименование);
			Запрос.УстановитьПараметр("ВидНоменклатуры", ВидНоменклатуры);
			Выборка = Запрос.Выполнить().Выбрать();
			
			Если Выборка.Следующий() Тогда
				Результат = Выборка.Ссылка;
			КонецЕсли;		
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(Результат) Тогда
			Если ТипНоменклатуры = "П" Тогда
				НаименованиеГруппы = "Созданная при загрузке электронных документов (Услуги)";
			Иначе
				НаименованиеГруппы = "Созданная при загрузке электронных документов (Товары)";
			КонецЕсли; 
			
			Запрос = Новый Запрос;
