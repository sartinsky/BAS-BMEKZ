		ПО НалоговаяНакладнаяОС.Ссылка = ВТ_Подготовленная.НалоговаяНакладная

СГРУППИРОВАТЬ ПО
	НалоговаяНакладнаяОС.СтавкаНДС,
	ВЫРАЗИТЬ(НалоговаяНакладнаяОС.ОсновноеСредство.НаименованиеПолное КАК СТРОКА(1000)),
	ВЫБОР
		КОГДА НалоговаяНакладнаяОС.КодУКТВЭД.Вид = ЗНАЧЕНИЕ(Перечисление.ВидыКодовДляНалоговойНакладной.КодУслуги)
			ТОГДА ""
		ИНАЧЕ ЕСТЬNULL(НалоговаяНакладнаяОС.КодУКТВЭД.Код, "")
	КОНЕЦ

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВЫРАЗИТЬ(НалоговаяНакладнаяНМА.НематериальныйАктив.НаименованиеПолное КАК СТРОКА(1000)),
	ВЫБОР
		КОГДА НалоговаяНакладнаяНМА.КодУКТВЭД.Вид = ЗНАЧЕНИЕ(Перечисление.ВидыКодовДляНалоговойНакладной.КодУслуги)
			ТОГДА ""
		ИНАЧЕ ЕСТЬNULL(НалоговаяНакладнаяНМА.КодУКТВЭД.Код, "")
	КОНЕЦ,
	"шт",
	НалоговаяНакладнаяНМА.СтавкаНДС,
	МАКСИМУМ(НалоговаяНакладнаяНМА.НомерСтроки)
ИЗ
	Документ.НалоговаяНакладная.НМА КАК НалоговаяНакладнаяНМА
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_Подготовленная КАК ВТ_Подготовленная
		ПО НалоговаяНакладнаяНМА.Ссылка = ВТ_Подготовленная.НалоговаяНакладная

СГРУППИРОВАТЬ ПО
	НалоговаяНакладнаяНМА.СтавкаНДС,
	ВЫРАЗИТЬ(НалоговаяНакладнаяНМА.НематериальныйАктив.НаименованиеПолное КАК СТРОКА(1000)),
	ВЫБОР
		КОГДА НалоговаяНакладнаяНМА.КодУКТВЭД.Вид = ЗНАЧЕНИЕ(Перечисление.ВидыКодовДляНалоговойНакладной.КодУслуги)
			ТОГДА ""
		ИНАЧЕ ЕСТЬNULL(НалоговаяНакладнаяНМА.КодУКТВЭД.Код, "")
	КОНЕЦ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Подготовленная.Номенклатура,
	ВЫБОР
		КОГДА ВТ_Подготовленная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
				И ВТ_Подготовленная.Сводная
			ТОГДА ""
		ИНАЧЕ ВТ_Подготовленная.КодУКТВЭД
	КОНЕЦ КАК КодУКТВЭД,
	ВЫБОР
		КОГДА ВТ_Подготовленная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
				И ВТ_Подготовленная.Сводная
			ТОГДА "грн"
		ИНАЧЕ ВТ_Подготовленная.ЕдИзмНаименование
	КОНЕЦ КАК ЕдИзмНаименование,
	ВЫБОР
		КОГДА ВТ_Подготовленная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
				И ВТ_Подготовленная.Сводная
			ТОГДА ""
		ИНАЧЕ ВТ_Подготовленная.ЕдИзмКод
	КОНЕЦ КАК ЕдИзмКод,
	ВЫБОР
		КОГДА ВТ_Подготовленная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
				И ВТ_Подготовленная.Сводная
			ТОГДА 0
		ИНАЧЕ ВТ_Подготовленная.Количество
	КОНЕЦ КАК Количество,
	ВЫБОР
		КОГДА ВТ_Подготовленная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
				И ВТ_Подготовленная.Сводная
			ТОГДА 0
		ИНАЧЕ ВТ_Подготовленная.Цена
	КОНЕЦ КАК Цена,
	ВТ_Подготовленная.Сумма,
	ВЫБОР
		КОГДА ВТ_Подготовленная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
				И ВТ_Подготовленная.Сводная
			ТОГДА ""
		ИНАЧЕ ВТ_Подготовленная.СтавкаНДС
	КОНЕЦ КАК СтавкаНДС,
	ВЫБОР
		КОГДА ВТ_Подготовленная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
				И ВТ_Подготовленная.Сводная
			ТОГДА ""
		ИНАЧЕ ВТ_Подготовленная.КодЛьготы
	КОНЕЦ КАК КодЛьготы,
	ВТ_Подготовленная.ВалютаДокумента,
	ВТ_Подготовленная.НеЯвляетсяРезидентом,
	ВТ_Подготовленная.Дата,
	ВЫБОР
		КОГДА ВТ_Подготовленная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
				И ВТ_Подготовленная.Сводная
			ТОГДА 0
		ИНАЧЕ ВТ_Подготовленная.ИзменениеЦены
	КОНЕЦ КАК ИзменениеЦены,
	ВЫБОР
		КОГДА ВТ_Подготовленная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
				И ВТ_Подготовленная.Сводная
			ТОГДА ""
		ИНАЧЕ ВТ_Подготовленная.ИзменениеКоличества
	КОНЕЦ КАК ИзменениеКоличества,
	ВТ_Подготовленная.Причина,
	ВЫБОР
		КОГДА ВТ_Подготовленная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
				И ВТ_Подготовленная.Сводная
			ТОГДА ""
		ИНАЧЕ ВТ_Подготовленная.ПризнакИмпортного
	КОНЕЦ КАК ПризнакИмпортного,
	ВЫБОР
		КОГДА ВТ_Подготовленная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
				И ВТ_Подготовленная.Сводная
			ТОГДА ""
		ИНАЧЕ ВТ_Подготовленная.КодДКПП
	КОНЕЦ КАК КодДКПП,
	ВТ_Подготовленная.НалоговаяНакладная,
	ЕСТЬNULL(ВТ_ДанныеТЧНалоговойНакладной.НомерСтроки, "") КАК НомерСтроки,
	ВТ_Подготовленная.НомерСтроки КАК НомерСтрокиДокумента
ИЗ
