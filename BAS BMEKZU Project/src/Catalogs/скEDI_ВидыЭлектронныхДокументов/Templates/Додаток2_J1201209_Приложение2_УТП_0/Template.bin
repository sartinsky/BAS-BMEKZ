		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Приложение2КНалоговойНакладной.Ссылка,
	ЕСТЬNULL(ОрганизацияEDI.КодЕДРПОУ, "") КАК КодЕДРПОУОрганизации,
	ЕСТЬNULL(КодыОрганизации.НалоговаяИнспекцияКод, "") КАК КодНалоговойИнспекции,
	Приложение2КНалоговойНакладной.Дата,
	Приложение2КНалоговойНакладной.Номер,
	Приложение2КНалоговойНакладной.ВидОперации,
	Приложение2КНалоговойНакладной.ТипПричиныНевыдачиПокупателю,
	ВЫБОР
		КОГДА Приложение2КНалоговойНакладной.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.РаботыОтНерезидентаКорректировка)
			ТОГДА ""
		ИНАЧЕ ЕСТЬNULL(Приложение2КНалоговойНакладной.НалоговаяНакладная.ОбособленноеПодразделение.Префикс, "")
	КОНЕЦ КАК ФилиалПродавца,
	ВЫБОР
		КОГДА Приложение2КНалоговойНакладной.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.РаботыОтНерезидентаКорректировка)
			ТОГДА ВЫБОР
					КОГДА Приложение2КНалоговойНакладной.Контрагент.ДополнятьНаименованиеАдресДаннымиГоловногоКонтрагентаВНН
						ТОГДА (ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.Контрагент.ГоловнойКонтрагент.НаименованиеПолное КАК СТРОКА(1000))) + ", " + (ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.Контрагент.НаименованиеПолное КАК СТРОКА(1000)))
					ИНАЧЕ ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.Организация.НаименованиеПолное КАК СТРОКА(1000))
				КОНЕЦ + ВЫБОР
					КОГДА ЕСТЬNULL(ЮрАдресКонтрагента.Страна, "") &lt;&gt; ""
						ТОГДА ", " + ЮрАдресКонтрагента.Страна
					ИНАЧЕ ""
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА ЕСТЬNULL(Приложение2КНалоговойНакладной.Организация.ГоловнаяОрганизация, ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)) &lt;&gt; ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)
					ТОГДА ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.Организация.ГоловнаяОрганизация.НаименованиеПолное КАК СТРОКА(1000))
				ИНАЧЕ ""
			КОНЕЦ + (ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.Организация.НаименованиеПолное КАК СТРОКА(1000))) + ЕСТЬNULL(ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.НалоговаяНакладная.ОбособленноеПодразделение.НаименованиеДляНалоговыхДокументов КАК СТРОКА(1000)), "")
	КОНЕЦ КАК ОрганизацияНаименование,
	ВЫБОР
		КОГДА Приложение2КНалоговойНакладной.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.РаботыОтНерезидентаКорректировка)
			ТОГДА "500000000000"
		ИНАЧЕ ЕСТЬNULL(КодыОрганизации.ИНН, "")
	КОНЕЦ КАК ИННОрганизации,
	ВЫБОР
		КОГДА Приложение2КНалоговойНакладной.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.РаботыОтНерезидентаКорректировка)
				ИЛИ Приложение2КНалоговойНакладной.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
			ТОГДА ВЫБОР
					КОГДА ЕСТЬNULL(Приложение2КНалоговойНакладной.Организация.ГоловнаяОрганизация, ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)) &lt;&gt; ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)
						ТОГДА (ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.Организация.ГоловнаяОрганизация.НаименованиеПолное КАК СТРОКА(1000))) + (ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.Организация.НаименованиеПолное КАК СТРОКА(1000)))
					ИНАЧЕ ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.Организация.НаименованиеПолное КАК СТРОКА(1000))
				КОНЕЦ + ВЫБОР
					КОГДА ЕСТЬNULL(Приложение2КНалоговойНакладной.НалоговаяНакладная.ОбособленноеПодразделение, ЗНАЧЕНИЕ(Справочник.ОбособленныеПодразделенияОрганизаций.ПустаяСсылка)) &lt;&gt; ЗНАЧЕНИЕ(Справочник.ОбособленныеПодразделенияОрганизаций.ПустаяСсылка)
						ТОГДА ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.НалоговаяНакладная.ОбособленноеПодразделение.НаименованиеДляНалоговыхДокументов КАК СТРОКА(1000))
						Иначе ""
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА Приложение2КНалоговойНакладной.ТипПричиныНевыдачиПокупателю = 1
					ТОГДА "Неплатник"
				КОГДА Приложение2КНалоговойНакладной.ТипПричиныНевыдачиПокупателю = 7
					ТОГДА (ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.Контрагент.НаименованиеПолное КАК СТРОКА(1000))) + ВЫБОР
							КОГДА ЕСТЬNULL(ЮрАдресКонтрагента.Страна, "") = ""
								ТОГДА ""
							ИНАЧЕ ", " + ЮрАдресКонтрагента.Страна
						КОНЕЦ
				КОГДА Приложение2КНалоговойНакладной.Контрагент.ИНН = ""
					ТОГДА "Неплатник"
				ИНАЧЕ ВЫРАЗИТЬ(Приложение2КНалоговойНакладной.Контрагент.НаименованиеПолное КАК СТРОКА(1000))
			КОНЕЦ
	КОНЕЦ КАК КонтрагентНаименование,
	ВЫБОР
		КОГДА Приложение2КНалоговойНакладной.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.РаботыОтНерезидентаКорректировка)
				ИЛИ Приложение2КНалоговойНакладной.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.УсловнаяПродажаВозврат)
			ТОГДА ВЫБОР
					КОГДА Приложение2КНалоговойНакладной.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.РаботыОтНерезидентаКорректировка)
						ТОГДА ЕСТЬNULL(КодыОрганизации.ИНН, "")
					ИНАЧЕ ВЫБОР
							КОГДА Приложение2КНалоговойНакладной.НалоговаяНакладная.Сводная
								ТОГДА "600000000000"
							ИНАЧЕ "400000000000"
						КОНЕЦ
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА Приложение2КНалоговойНакладной.ТипПричиныНевыдачиПокупателю = 1
					ТОГДА "100000000000"
				КОГДА Приложение2КНалоговойНакладной.ТипПричиныНевыдачиПокупателю = 12
					ТОГДА "200000000000"
				КОГДА Приложение2КНалоговойНакладной.ТипПричиныНевыдачиПокупателю = 7
					ТОГДА "300000000000"
				КОГДА Приложение2КНалоговойНакладной.Контрагент.ИНН = ""
					ТОГДА "100000000000"
				ИНАЧЕ Приложение2КНалоговойНакладной.Контрагент.ИНН
			КОНЕЦ
	КОНЕЦ КАК КонтрагентИНН,
	ВЫБОР
		КОГДА Приложение2КНалоговойНакладной.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПриложение2КНалоговойНакладной.РаботыОтНерезидентаКорректировка)
			ТОГДА ВЫБОР
					КОГДА Приложение2КНалоговойНакладной.НалоговаяНакладная.ОбособленноеПодразделение.Ссылка &lt;&gt; ЗНАЧЕНИЕ(Справочник.ОбособленныеПодразделенияОрганизаций.ПустаяСсылка)
						ТОГДА ЕСТЬNULL(Приложение2КНалоговойНакладной.НалоговаяНакладная.ОбособленноеПодразделение.Префикс, "")
					ИНАЧЕ ""
				КОНЕЦ
		ИНАЧЕ ЕСТЬNULL(Приложение2КНалоговойНакладной.Контрагент.КодФилиала, "")
	КОНЕЦ КАК ФилиалПокупателя,
	ПОДСТРОКА(ЕСТЬNULL(ФИОФизЛицСрезПоследних.Имя, ""), 1, 1) + ". " + ПОДСТРОКА(ЕСТЬNULL(ФИОФизЛицСрезПоследних.Отчество, ""), 1, 1) + ". " + ЕСТЬNULL(ФИОФизЛицСрезПоследних.Фамилия, "") КАК КтоВыписалНалоговуюНакладнуюНаименование,
	Приложение2КНалоговойНакладной.КтоВыписалНалоговуюНакладную.КодПоДРФО,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.Сумма, 0) КАК СуммаСУчетомНДС,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаНДС, 0) КАК СуммаНДС,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаБезНДС, 0) КАК СуммаБезНДС,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаНДС_901, 0) КАК СуммаНДС_901,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаНДС_902, 0) КАК СуммаНДС_902,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаНДС_903, 0) КАК СуммаНДС_903,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаНДС_7, 0) КАК СуммаНДС_7,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаНДС_20, 0) КАК СуммаНДС_20,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.Сумма_20, 0) КАК Сумма_20,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.Сумма_7, 0) КАК Сумма_7,
	ЕСТЬNULL(КодыОрганизации.НалоговаяИнспекцияРегион, "") КАК НалоговаяИнспекцияРегион,
	ЕСТЬNULL(КодыОрганизации.НалоговаяИнспекцияРайон, "") КАК НалоговаяИнспекцияРайон,
	Приложение2КНалоговойНакладной.ДоговорКонтрагента.МногостороннееСоглашениеОРазделеПродукции,
	Приложение2КНалоговойНакладной.СпецРежимНалогообложения,
	Приложение2КНалоговойНакладной.НалоговаяНакладная.Дата,
	Приложение2КНалоговойНакладной.НалоговаяНакладная.Номер,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.ИзменениеСуммыНДС, 0) КАК ИзменениеСуммыНДС,
	Приложение2КНалоговойНакладной.РегистрируетсяВЕРННПокупателем,
