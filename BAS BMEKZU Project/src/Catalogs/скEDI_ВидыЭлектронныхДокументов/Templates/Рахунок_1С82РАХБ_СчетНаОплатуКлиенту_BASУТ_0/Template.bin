		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	СчетНаОплатуКлиенту.Дата КАК Дата,
	" від " КАК ДоговорДатаТекст,
	ЕСТЬNULL(СчетНаОплатуКлиенту.Договор.Дата, ДАТАВРЕМЯ(1, 1, 1)) КАК ДоговорДата,
	СчетНаОплатуКлиенту.Договор.Номер КАК ДоговорНомер,
	"№ " КАК ДоговорНомерТекст,
	"Договір:" КАК ДоговорТекст,
	ЕСТЬNULL(ЮрАдресОрганизации.Представление, "") КАК ЮрАдресОрганизации,
	ЕСТЬNULL(КодыОрганизаций.ИНН, "") КАК ИНН,
	ЕСТЬNULL(ОрганизацияEDI.Код, "") КАК КодПоЕДРПОУ,
	ЕСТЬNULL(КодыОрганизаций.НомерСвидетельства, "") КАК НомерСвидетельства,
	ЕСТЬNULL(СчетНаОплатуКлиенту.БанковскийСчет.Банк.Код, "") КАК МФО,
	СчетНаОплатуКлиенту.Организация.НаименованиеПолное КАК ОрганизацияНаименование,
	ЕСТЬNULL(СчетНаОплатуКлиенту.БанковскийСчет.Банк.Наименование, "") КАК БанкНаименование,
	ЕСТЬNULL(СчетНаОплатуКлиенту.БанковскийСчет.НомерСчета, "") КАК НомерСчета,
	ЕСТЬNULL(ТелефонПоЮрАдресуОрганизации.Представление, "") КАК ТелефонПоЮрАдресуОрганизации,
	СчетНаОплатуКлиенту.Ссылка,
	ЕСТЬNULL(ЮрАдресКонтрагента.Представление, "") КАК ЮрАресКонтрагента,
	ЕСТЬNULL(ТелефонПоЮрАдресуКонтрагента.Представление, "") КАК ТелефонПоЮрАдресуКонтрагент,
	СчетНаОплатуКлиенту.Контрагент.НаименованиеПолное КАК НаименованиеКонтрагента,
	СчетНаОплатуКлиенту.Контрагент.КодФилиала КАК КодФилиалаКонтрагента,
	СчетНаОплатуКлиенту.Контрагент.КодПоЕДРПОУ КАК КодЕДРПОУКонтрагента,
	СчетНаОплатуКлиенту.ПредставительОрганизации.Наименование КАК ПредставительОрганизацииНаименование,
	СчетНаОплатуКлиенту.КонтактноеЛицо.Наименование КАК ОтветственноеЛицоКонтрагента,
	ЕСТЬNULL(ДанныеПоТабличнойЧастиСчет.СуммаНДС, 0) КАК СуммаНДС,
	ЕСТЬNULL(ДанныеПоТабличнойЧастиСчет.СуммаНДС, 0) КАК СуммаНДСТекст,
	"код за ЄДРПОУ " КАК ЕДРПОУОрганизацииТекст,
	"код за ЄДРПОУ " КАК ЕДРПОУКонтрагентаТекст,
	ЕСТЬNULL(УчетнаяПолитика.СхемаНалогообложенияНаименование, "") КАК СхемаНалогообложения,
	ДанныеПоТабличнойЧастиСчет.Сумма,
	ДанныеПоТабличнойЧастиСчет.Сумма КАК СуммаТекст,
	ЕСТЬNULL(СчетНаОплатуКлиенту.Контрагент.ИННПлательщикаНДС, "") КАК КонтрагентИНН,
	ЕСТЬNULL(СчетНаОплатуКлиенту.Контрагент.НомерСвидетельстваПлательщикаНДС, 0) КАК КонтрагентНомерСвидетельства,
	ЕСТЬNULL(ДанныеПоТабличнойЧастиСчет.СуммаБезНДС, 0) КАК СуммаБезНДС,
	ЕСТЬNULL(ДанныеПоТабличнойЧастиСчет.СуммаБезНДС, 0) КАК СуммаБезНДС_Текст,
	СчетНаОплатуКлиенту.Номер,
	СчетНаОплатуКлиенту.Организация,
