		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	НалоговаяНакладная.Ссылка,
	ВЫБОР
		КОГДА (НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.ОблагаемыеОперации)
				ИЛИ НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.ОсвобожденныеОперации)
				ИЛИ НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.УсловнаяПродажа)
				ИЛИ НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.УсловнаяПродажаСписаниеОС))
				И НалоговаяНакладная.Сводная
			ТОГДА 1
		ИНАЧЕ ""
	КОНЕЦ КАК Сводная,
	ВЫБОР
		КОГДА НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.ИтоговаяРозницаОсвобожденныеОперации)
				ИЛИ НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.ОсвобожденныеОперации)
				ИЛИ НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.РозницаКонрагентуОсвобожденныеОперации)
			ТОГДА "Без ПДВ"
		ИНАЧЕ ""
	КОНЕЦ КАК Освобожденная,
	ВЫБОР
		КОГДА НалоговаяНакладная.ТипПричиныНевыдачиПокупателю &lt;&gt; 0
			ТОГДА 1
		ИНАЧЕ ""
	КОНЕЦ КАК ВсеЭкзОстаютсяУПродавца,
	ВЫБОР
		КОГДА НалоговаяНакладная.ДоговорКонтрагента.МногостороннееСоглашениеОРазделеПродукции
			ТОГДА 5
		КОГДА НалоговаяНакладная.СпецРежимНалогообложения = 0
			ТОГДА " "
		ИНАЧЕ НалоговаяНакладная.СпецРежимНалогообложения
	КОНЕЦ КАК СпецРежим,
	ЕСТЬNULL(ОрганизацияEDI.КодЕДРПОУ, "") КАК КодЕДРПОУОрганизации,
	ЕСТЬNULL(КодыОрганизации.НалоговаяИнспекцияКод, "") КАК КодНалоговойИнспекции,
	НалоговаяНакладная.Дата,
	НалоговаяНакладная.Номер,
	НалоговаяНакладная.ВидОперации,
	ВЫБОР
		КОГДА НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.РаботыОтНерезидента)
			ТОГДА ""
		ИНАЧЕ ЕСТЬNULL(НалоговаяНакладная.ОбособленноеПодразделение.Префикс, "")
	КОНЕЦ КАК ФилиалПродавца,
	ВЫБОР
		КОГДА НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.РаботыОтНерезидента)
			ТОГДА ВЫБОР
					КОГДА НалоговаяНакладная.Контрагент.ДополнятьНаименованиеАдресДаннымиГоловногоКонтрагентаВНН
						ТОГДА (ВЫРАЗИТЬ(НалоговаяНакладная.Контрагент.ГоловнойКонтрагент.НаименованиеПолное КАК СТРОКА(1000))) + ", " + (ВЫРАЗИТЬ(НалоговаяНакладная.Контрагент.НаименованиеПолное КАК СТРОКА(1000)))
					ИНАЧЕ ВЫРАЗИТЬ(НалоговаяНакладная.Контрагент.НаименованиеПолное КАК СТРОКА(1000))
				КОНЕЦ + ВЫБОР
					КОГДА ЕСТЬNULL(ЮрАдресКонтрагента.Страна, "") &lt;&gt; ""
						ТОГДА ", " + ЮрАдресКонтрагента.Страна
					ИНАЧЕ ""
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА ЕСТЬNULL(НалоговаяНакладная.Организация.ГоловнаяОрганизация, ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)) &lt;&gt; ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)
					ТОГДА ВЫРАЗИТЬ(НалоговаяНакладная.Организация.ГоловнаяОрганизация.НаименованиеПолное КАК СТРОКА(1000))
				ИНАЧЕ ""
			КОНЕЦ + (ВЫРАЗИТЬ(НалоговаяНакладная.Организация.НаименованиеПолное КАК СТРОКА(1000))) + ЕСТЬNULL(ВЫРАЗИТЬ(НалоговаяНакладная.ОбособленноеПодразделение.НаименованиеДляНалоговыхДокументов КАК СТРОКА(1000)), "")
	КОНЕЦ КАК ОрганизацияНаименование,
	ВЫБОР
		КОГДА НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.РаботыОтНерезидента)
			ТОГДА "500000000000"
		ИНАЧЕ ЕСТЬNULL(КодыОрганизации.ИНН, "")
	КОНЕЦ КАК ИННОрганизации,
	ВЫБОР
		КОГДА НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.РаботыОтНерезидента)
				ИЛИ НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.УсловнаяПродажа)
				ИЛИ НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.УсловнаяПродажаСписаниеОС)
			ТОГДА ВЫБОР
					КОГДА ЕСТЬNULL(НалоговаяНакладная.Организация.ГоловнаяОрганизация, ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)) &lt;&gt; ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)
						ТОГДА (ВЫРАЗИТЬ(НалоговаяНакладная.Организация.ГоловнаяОрганизация.НаименованиеПолное КАК СТРОКА(1000))) + (ВЫРАЗИТЬ(НалоговаяНакладная.Организация.НаименованиеПолное КАК СТРОКА(1000)))
					ИНАЧЕ ВЫРАЗИТЬ(НалоговаяНакладная.Организация.НаименованиеПолное КАК СТРОКА(1000))
				КОНЕЦ + ВЫБОР
					КОГДА ЕСТЬNULL(НалоговаяНакладная.ОбособленноеПодразделение, ЗНАЧЕНИЕ(Справочник.ОбособленныеПодразделенияОрганизаций.ПустаяСсылка)) &lt;&gt; ЗНАЧЕНИЕ(Справочник.ОбособленныеПодразделенияОрганизаций.ПустаяСсылка)
						ТОГДА ВЫРАЗИТЬ(НалоговаяНакладная.ОбособленноеПодразделение.НаименованиеДляНалоговыхДокументов КАК СТРОКА(1000))
					ИНАЧЕ ""
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА НалоговаяНакладная.ТипПричиныНевыдачиПокупателю = 1
					ТОГДА "Неплатник"
				КОГДА НалоговаяНакладная.ТипПричиныНевыдачиПокупателю = 7
					ТОГДА (ВЫРАЗИТЬ(НалоговаяНакладная.Контрагент.НаименованиеПолное КАК СТРОКА(1000))) + ВЫБОР
							КОГДА ЕСТЬNULL(ЮрАдресКонтрагента.Страна, "") = ""
								ТОГДА ""
							ИНАЧЕ ", " + ЮрАдресКонтрагента.Страна
						КОНЕЦ
				КОГДА НалоговаяНакладная.Контрагент.ИНН = ""
					ТОГДА "Неплатник"
				ИНАЧЕ ВЫРАЗИТЬ(НалоговаяНакладная.Контрагент.НаименованиеПолное КАК СТРОКА(1000))
			КОНЕЦ
	КОНЕЦ КАК КонтрагентНаименование,
	ВЫБОР
		КОГДА НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.РаботыОтНерезидента)
				ИЛИ НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.УсловнаяПродажа)
				ИЛИ НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.УсловнаяПродажаСписаниеОС)
			ТОГДА ВЫБОР
					КОГДА НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.РаботыОтНерезидента)
						ТОГДА ЕСТЬNULL(КодыОрганизации.ИНН, "")
					ИНАЧЕ ВЫБОР
							КОГДА НалоговаяНакладная.Сводная
								ТОГДА "600000000000"
							ИНАЧЕ "400000000000"
						КОНЕЦ
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА НалоговаяНакладная.ТипПричиныНевыдачиПокупателю = 1
					ТОГДА "100000000000"
				КОГДА НалоговаяНакладная.ТипПричиныНевыдачиПокупателю = 12
					ТОГДА "200000000000"
				КОГДА НалоговаяНакладная.ТипПричиныНевыдачиПокупателю = 7
					ТОГДА "300000000000"
				КОГДА НалоговаяНакладная.Контрагент.ИНН = ""
					ТОГДА "100000000000"
				ИНАЧЕ НалоговаяНакладная.Контрагент.ИНН
			КОНЕЦ
	КОНЕЦ КАК КонтрагентИНН,
	ВЫБОР
		КОГДА НалоговаяНакладная.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийНалоговаяНакладная.РаботыОтНерезидента)
			ТОГДА ВЫБОР
					КОГДА НалоговаяНакладная.ОбособленноеПодразделение.Ссылка &lt;&gt; ЗНАЧЕНИЕ(Справочник.ОбособленныеПодразделенияОрганизаций.ПустаяСсылка)
						ТОГДА ЕСТЬNULL(НалоговаяНакладная.ОбособленноеПодразделение.Префикс, "")
					ИНАЧЕ ""
				КОНЕЦ
		ИНАЧЕ ЕСТЬNULL(НалоговаяНакладная.Контрагент.КодФилиала, "")
	КОНЕЦ КАК ФилиалПокупателя,
	ПОДСТРОКА(ЕСТЬNULL(ФИОФизЛицСрезПоследних.Имя, ""), 1, 1) + ". " + ПОДСТРОКА(ЕСТЬNULL(ФИОФизЛицСрезПоследних.Отчество, ""), 1, 1) + ". " + ЕСТЬNULL(ФИОФизЛицСрезПоследних.Фамилия, "") КАК КтоВыписалНалоговуюНакладнуюНаименование,
	НалоговаяНакладная.КтоВыписалНалоговуюНакладную.КодПоДРФО,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.Сумма, 0) КАК СуммаСУчетомНДС,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаНДС, 0) КАК СуммаНДС,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаБезНДС, 0) КАК СуммаБезНДС,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.Сумма_901, 0) КАК Сумма_901,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.Сумма_902, 0) КАК Сумма_902,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.Сумма_903, 0) КАК Сумма_903,
	ЕСТЬNULL(ДанныеПоТабличнойЧасти.СуммаНДС_7, 0) КАК СуммаНДС_7,
