				ИмяПКОВидСубконто = "ВидыСубконтоХозрасчетныеСтр";
				ИмяПКО = "НазначениеИспользования";
			ИначеЕсли ВидСубконтоИсточника = ВидыСубконто.Партии Тогда
				ВидСубконто = "00031"; // Партии малоценки в эксплуатации
				ИмяПКОВидСубконто = "ВидыСубконтоХозрасчетныеСтр";
				ИмяПКО = "ПартияМЦПоУмолчанию";
			КонецЕсли;
		ИначеЕсли Лев(Счет.Код,2) = "02" Тогда			
			Если ВидСубконтоИсточника = ВидыСубконто.Партии Тогда
				Если Счет = СчетПоКоду("024") Тогда
					ИмяПКО = "ПартияНаЗабалансе";
				Иначе
					Отказ = 1;
				КонецЕсли;
			КонецЕсли;
		ИначеЕсли ВидСубконтоИсточника = ВидыСубконто.Заказы Тогда
			
			ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
			ИсходящиеДанные.Установить("ВидДоговора", 		   Параметры.ПараметрыАлгоритмов.Получить("ВзаиморасчетыВидДоговора"));
			ИсходящиеДанные.Установить("ВалютаВзаиморасчетов", Параметры.ПараметрыАлгоритмов.Получить("ВзаиморасчетыВалютаВзаиморасчетов"));
			
			Если Субконто.Вид() = "Договор" Тогда
				ИмяПКО = "ДоговорыКонтрагентов";
			Иначе
				ИмяПКО = "Заказы";
				
				Контрагент = "";
				Для Инд = 1 по 3 Цикл
					Контрагент = Параметры.ПараметрыАлгоритмов.Получить("Субконто" + Инд);
					Если ПустоеЗначение(Контрагент) = 0 Тогда
						Если ТипЗначенияСтр(Контрагент) = "Справочник" Тогда
							Если Контрагент.Вид() = "Контрагенты" Тогда
								Прервать;
							КонецЕсли;
						КонецЕсли;
					КонецЕсли;
				КонецЦикла;
				ИсходящиеДанные.Установить("Владелец", Контрагент);
			КонецЕсли;
			
		ИначеЕсли ВидСубконтоИсточника = ВидыСубконто.НалогиОтчисления Тогда
			ИмяПКО = "Налоги";
		ИначеЕсли ВидСубконтоИсточника = ВидыСубконто.ВидыДеятельности Тогда
			Отказ = 1;// в общем случае не переносим
		Иначе
			
			Если ВидСубконтоИсточника = ВидыСубконто.Кредиты Тогда
				ВидСубконто = "00011"; // договоры (по кредиту)
				ИмяПКОВидСубконто = "ВидыСубконтоХозрасчетныеСтр";
				ИмяПКО = "КредитВДоговор";
				ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
				ИсходящиеДанные.Установить("ВалютаВзаиморасчетов", Параметры.ПараметрыАлгоритмов.Получить("ВзаиморасчетыВалютаВзаиморасчетов"));
			//ИначеЕсли ВидСубконтоИсточника = ВидыСубконто.БланкиСтрогойОтчетности Тогда
			//	ВидСубконто = "00001"; // Бланки строгой отчетности
			//	ИмяПКОВидСубконто = "ВидыСубконтоХозрасчетныеСтр";
			//	ИмяПКО = "БланкиСтрогойОтчетностиВНоменклатуру";
			КонецЕсли;
			
			ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
			ИсходящиеДанные.Установить("Владелец", ВидСубконто);
			
		КонецЕсли;
		
	КонецЕсли;

	НомерПКОСубконто = НайтиПравило(Субконто, ИмяПКО);

	Если НомерПКОСубконто = 0 Тогда
		Отказ = 1;
	КонецЕсли;
	
Иначе
	Отказ = 1;
КонецЕсли;</Текст>
		</Алгоритм>
