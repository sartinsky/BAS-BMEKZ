Пока СчетаБУ.ПолучитьСтроку() = 1 Цикл
	Счет = СчетаБУ.СчетБУ;	
	Параметры.ПараметрыАлгоритмов.Установить("ТекущийСчет", Счет);
 	КоличествоУровней = 3;

	Сч282 = СчетПоКоду("282");
	//Сч283 = СчетПоКоду("283");
	Сч285 = СчетПоКоду("285");

	
	БухИт = СоздатьОбъект("БухгалтерскиеИтоги");
	БухИт.ИспользоватьРазделительУчета(Параметры.Организация);
	БухИт.ИспользоватьСубконто(ВидыСубконто.МестаХранения);
	БухИт.ИспользоватьСубконто(ВидыСубконто.ТМЦ);
	БухИт.ИспользоватьСубконто(ВидыСубконто.Партии);

	Параметры.ПараметрыАлгоритмов.Установить("БухИт", БухИт);
	Параметры.ПараметрыАлгоритмов.Установить("Проводки", Проводки);
	Параметры.ПараметрыАлгоритмов.Установить("КоличествоУровней", КоличествоУровней);

	БухИт.ВыполнитьЗапрос(НачГода(Параметры.ДатаВводаОстатков), Параметры.ДатаВводаОстатков, Счет);
	БухИт.ВыбратьСчета();
	
	СуммаБУ = СуммаБУ + БухИт.СКД();
		Пока БухИт.ПолучитьСчет() = 1 Цикл
		Если ПолучитьОстаткиПоСчету() = 0 Тогда
			Продолжить;
		КонецЕсли;
		БухИт.ВыбратьСубконто(1);//МестаХраненя
		Пока БухИт.ПолучитьСубконто(1) = 1 Цикл
			Если БухИт.Субконто(1).ЭтоГруппа() = 1 Тогда
				Продолжить;
			КонецЕсли;
			
			Если ПолучитьОстаткиПоСчету() = 0 Тогда
				Продолжить;
			КонецЕсли;
			
			БухИт.ВыбратьСубконто(2);//ТМЦ
			Пока БухИт.ПолучитьСубконто(2) = 1 Цикл 
				Если БухИт.Субконто(2).ЭтоГруппа() = 1 Тогда
					Продолжить;
				КонецЕсли;
				Если ПолучитьОстаткиПоСчету() = 0 Тогда
					Продолжить;
				КонецЕсли;
				БухИт.ВыбратьСубконто(3);// пратии
				Пока БухИт.ПолучитьСубконто(3) = 1 Цикл
					Если БухИт.Субконто(3).ЭтоГруппа() = 1 Тогда
						Продолжить;
					КонецЕсли;
					Если ПолучитьОстаткиПоСчету() = 1 Тогда
						Параметры.ПараметрыАлгоритмов.Установить("ТекНомерСубконто", 3);
						ДобавитьПроводку();
						
						Если БухИт.Счет = Сч282 Тогда
							Если БухИт.Субконто(1).СуммовойУчет = 1 Тогда
								Проводки.КодСчетаДт = "2822";
							Иначе
								Проводки.КодСчетаДт = "2821";
							КонецЕсли;
						ИначеЕсли БухИт.Счет = Сч285 Тогда
							Если БухИт.Субконто(1).СуммовойУчет = 1 Тогда
								Проводки.КодСчетаКт = "2852";
							Иначе
								Проводки.КодСчетаКт = "2851";
							КонецЕсли;
						КонецЕсли;
						Проводки.СуммаНУ = Проводки.Сумма;
						буфСчет = СоздатьОбъект("Счет.Основной");
						Если буфСчет.НайтиПоКоду("Н"+БухИт.Счет.Код) = 1 Тогда 
							
							ИтНУ = СоздатьОбъект("БухгалтерскиеИтоги");
							ИтНУ.ИспользоватьРазделительУчета(Параметры.Организация);
							
							ИтНУ.ИспользоватьСубконто(БухИт.Счет.ВидСубконто(1));
							ИтНУ.ИспользоватьСубконто(БухИт.Счет.ВидСубконто(2));
							ИтНУ.ИспользоватьСубконто(БухИт.Счет.ВидСубконто(3));
							
							ИтНУ.ВыполнитьЗапрос(НачГода(Параметры.ДатаВводаОстатков), Параметры.ДатаВводаОстатков, СчетПоКоду("Н"+БухИт.Счет.Код));
							ИтНУ.ВыбратьСубконто(1);
							Если ИтНУ.ПолучитьСубконто(1,,БухИт.Субконто(1)) = 1 Тогда
								ИтНУ.ВыбратьСубконто(2);
								Если ИтНУ.ПолучитьСубконто(2,,БухИт.Субконто(2)) = 1 Тогда
									ИтНУ.ВыбратьСубконто(3);
									Если ИтНУ.ПолучитьСубконто(3,,БухИт.Субконто(3)) = 1 Тогда
										ИтНУ.ВыбратьСчета();
										Если ИтНУ.ПолучитьСчет(,буфСчет.ТекущийСчет()) = 1 Тогда
											Проводки.СуммаНУ = Проводки.Сумма + (ИтНУ.СКД("1")-ИтНУ.СКК("1"));
										КонецЕсли;	
									КонецЕсли;
								КонецЕсли;
							КонецЕсли;
						КонецЕсли;
					КонецЕсли;
				КонецЦикла;
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
КонецЦикла;

СписокЗапасов = СоздатьОбъект("ТаблицаЗначений");
СписокЗапасов.НоваяКолонка("СчетБУ");
СписокЗапасов.НоваяКолонка("Субконто");
СписокЗапасов.НоваяКолонка("Сумма");
СписокЗапасов.НоваяКолонка("СуммаНУ");
СписокЗапасов.НоваяКолонка("Количество");
Списокзапасов.НоваяКолонка("Склад");
СписокЗапасов.НоваяКолонка("Номенклатура");
СписокЗапасов.НоваяКолонка("НалоговоеНазначение");

ОкрВверх = 1;
Если СуммаБУ &lt;&gt; 0 Тогда

	
	БезНДС =  СоздатьОбъект("Справочник.НалогиОтчисления");
	Если БезНДС.НайтиПоКоду("БезНДС") = 1 Тогда
	    БезНДС = БезНДС.ТекущийЭлемент();
	КонецЕсли;
	
	Проводки.ВыбратьСтроки();
	Пока Проводки.ПолучитьСтроку() = 1 Цикл
		Если  ПустоеЗначение(Проводки.СубконтоДт)=0 Тогда
			Склад		= Проводки.СубконтоДт.ПолучитьЗначение(1,"Значение");
			Номенклатура= Проводки.СубконтоДт.ПолучитьЗначение(2,"Значение");
			Партия		= Проводки.СубконтоДт.ПолучитьЗначение(3,"Значение");
		Иначе
			Склад		= Проводки.СубконтоКт.ПолучитьЗначение(1,"Значение");
			Номенклатура= Проводки.СубконтоКт.ПолучитьЗначение(2,"Значение");
			Партия		= Проводки.СубконтоКт.ПолучитьЗначение(3,"Значение");
		КонецЕсли;
		
		НалоговоеНазначение = "ХозДеятельность";
		Если Параметры.ЕстьНДС = 1 Тогда
			Если Номенклатура.СтавкаНДС = БезНДС Тогда
				НалоговоеНазначение = "ХозДеятельностьНеОблагНДС";	
			КонецЕсли;
		КонецЕсли;


		СписокЗапасов.НоваяСтрока();
		Если  ПустоеЗначение(Проводки.СубконтоДт)=0  Тогда 
			СписокЗапасов.СчетБУ = Проводки.СчетДт;
			СписокЗапасов.Количество = Проводки.КоличествоДт;
		Иначе
			СписокЗапасов.СчетБУ = Проводки.СчетКт;
			СписокЗапасов.Количество = Проводки.КоличествоКт;
		КонецЕсли;
				
		СписокЗапасов.Сумма = Проводки.Сумма;
		СписокЗапасов.СуммаНУ = Проводки.СуммаНУ;
		СписокЗапасов.Номенклатура = Номенклатура;
		СписокЗапасов.Склад = Склад;
		СписокЗапасов.НалоговоеНазначение = НалоговоеНазначение;
	КонецЦикла;
КонецЕсли;


Если СписокЗапасов.КоличествоСтрок() &gt; 0 Тогда
	Если СчетНУ = СчетПоКоду("ЗАР") Тогда
		ВыгрузитьПоПравилу(,,СписокЗапасов,,"ВНО_ТоварыПоПродажнойЦене");
	Иначе
		ВыгрузитьПоПравилу(,, СписокЗапасов,, "ВНО_ЗапасыСебестоимость");
	КонецЕсли;
КонецЕсли;</Текст>
