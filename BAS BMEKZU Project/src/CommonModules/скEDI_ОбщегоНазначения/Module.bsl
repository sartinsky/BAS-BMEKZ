						ДополнительныеПараметры.ЖурналОперации = ЖурналОперации;
					Иначе
						ДополнительныеПараметры.ЖурналОперации = ДополнительныеПараметры.ЖурналОперации + "; " + ЖурналОперации;
					КонецЕсли;
				КонецЕсли;
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;
КонецПроцедуры

