				лОписаниеПоОтправке = лОписаниеПоОтправке + "
				|"
				+ НСтр("ru = 'Ссылка на документ: '; uk = 'Посилання на документ: '") + лСсылкаНаДокумент;
			КонецЕсли;
			
			лОписаниеПоОтправке = лОписаниеПоОтправке + "
			|
			|"
			+ НСтр("ru = 'Подписи: '; uk = 'Підписи: '");
