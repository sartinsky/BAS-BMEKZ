				ТелоДокумента,
				ИзображениеДокумента,
				Ошибка) Тогда
				
				Результат.ТелоДокумента = ТелоДокумента;
				Результат.ИзображениеДокумента = ИзображениеДокумента;
			Иначе
				Возврат Неопределено;
			КонецЕсли;
		Иначе
		КонецЕсли;
		
		Возврат Результат;
	Иначе
		Возврат Неопределено;
	КонецЕсли;
