				МассивСтатусов = Новый Массив;
				МассивСтатусов.Добавить(Статус1);
				МассивСтатусов.Добавить(Статус2);
				лпПараметрыВыводаФаксимиле.Вставить("StatusImageArr", МассивСтатусов);
			КонецЕсли;
		Иначе
			лпПараметрыВыводаФаксимиле.Вставить("StatusImage", Статус2);
		КонецЕсли;
	Иначе
		Если ЭлектронныйДокумент.Состояние = ПредопределенноеЗначение("Перечисление.скEDI_СостоянияЭлектронныхДокументов.ПолученоСообщениеОДоставке") Тогда
