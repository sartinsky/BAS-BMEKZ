		ДанныеВыбора.Добавить(ПредопределенноеЗначение("Перечисление.ВидыСправокОДоходах.Пенсия"));
		ДанныеВыбора.Добавить(ПредопределенноеЗначение("Перечисление.ВидыСправокОДоходах.Субсидия"));
